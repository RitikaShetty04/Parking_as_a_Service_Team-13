						<script>
							var firstAngularApp = angular
									.module('firstAngularApp', [])
									.controller(
											"personController",
											function($scope, $http, $location,
													$window) {

												$scope.signUp = function() {
													console
															.log("Inside angular function");
													if ($scope.password == $scope.retypePassword) {
														console
																.log("recaptcha"
																		+ grecaptcha
																				.getResponse());
														$http(
																{
																	method : 'POST',
																	url : '/sign_up',
																	data : {
																		"firstName" : $scope.firstName,
																		"lastName" : $scope.lastName,
																		"address" : $scope.address,
																		"city" : $scope.city,
																		"state" : $scope.state,
																		"zipCode" : $scope.zipCode,
																		"phoneNo" : $scope.phoneNo,
																		"email" : $scope.email,
																		"creditCardNumber" : $scope.creditCardNumber,
																		"password" : $scope.password,
																		"nameOnCard" : $scope.nameOnCard,
																		"securityCode" : $scope.securityCode,
																		"expiry" : $scope.expiry,
																		"signUpAs" : $scope.radioValue,
																		"g-recaptcha-response" : grecaptcha
																				.getResponse()
																	}

																})
																.success(
																		function(
																				response) {
																			window
																					.alert(JSON
																							.stringify(response));
																			if (response.signup == "Owner Success")
																				window
																						.alert("Signup successful! Login as owner to continue");
																			else if (response.signup == "Renter Success")
																				window
																						.alert("Signup successful! Login as renter to continue");
																			else if (response.signup == "User already exists")
																				window
																						.alert("Username already exists! Use another username to signup");
																			else
																				window.location = '/fail_signup';

																		})
																.error(
																		function(
																				error) {
																			window
																					.alert("error");
																		});
													} else {
														window
																.alert("Password mismatch");
													}
												};
												//--------------------------------------------------------------------------------------------------------
												$scope.renter_login = function() {
													$window.location = "/renterLogin";
												};
												//-------------------------------------------------------------------------------------------------------
												$scope.owner_login = function() {
													window.location = "/login";
												};
												//-------------------------------------------------------------------------------------------------------
												$scope.admin_login = function() {
													window.location = "/adminLogin";
												};
											});
