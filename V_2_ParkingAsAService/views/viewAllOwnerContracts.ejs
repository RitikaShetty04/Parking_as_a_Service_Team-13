							</li>
						</ul>
					</div>
					<div class="col-md-8 container-fluid mycontainer">
					<!-- wite code here -->
						<!--<div ng-include="templateView.template"></div>-->
<h3 text-align="center">All Contracts</h3>
<%=result%>
<table class="table">
<th>Billing Date</th>
<th>Address</th>
<th>Parking Start Time</th>
<th>Parking End Time</th>
<th>Duration</th>
<th>Total Amount</th>

    <% if (typeof rows !== "undefined") { %>
  <% for (var i = 0; i < rows.length; i++) { %>

<tr>				
<td> <%=rows[i].date%></td>
<td> <%=rows[i].address %></td>
<td> <%=rows[i].parkingStartTime %> hrs</td>
<td> <%=rows[i].parkingEndTime %> hrs</td>
<td> <%=rows[i].duration %> hrs</td>
<td> $ <%=rows[i].totalAmount %></td>
</tr>

	<% }%>
	<% }%>
</table>
     			
